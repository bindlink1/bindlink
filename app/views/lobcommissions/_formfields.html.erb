
<table class="table table-striped table-bordered table-condensed">
  <col width="15%" />
  <col width="10%" />
  <col width="20%" />
  <col width="20%" />
  <col width="35%" />
  <thead>
  <tr>
    <th>Actions</th>
    <th>State</th>
    <% if current_agent.isgeneralagent == "GA" %>
        <th>General Agency</th>
        <th>Producing Agency</th>
    <%else%>
        <th>New Rate</th>
        <th>Renewal Rate</th>
    <%end%>
    <th>Billing Type</th>
  </tr>
  </thead>
  <tbody>
  <tr>


    <td>
      <%= f.submit 'save', :remote => true, :class=>"btn btn-success btn-mini", :id=>"lobcommsub"%> <button class="btn btn-mini" id="lobcancel<%= @carrierlob.id %>" onclick='return false;' >cancel</button>
    </td>
    <td >
      <%= f.collection_select :state, State.all, :abbreviation, :abbreviation, {:include_blank => 'Select One'} %>
    </td>
    <td>
      <%= f.text_field :commission_rate,:autocomplete => :off,:class=>"input-medium", :placeholder =>"New Rate"  %>
    </td>
    <% if current_agent.isgeneralagent == "GA" %>
        <td>
          <%= f.text_field :producer_rate,:autocomplete => :off,:class=>"input-medium", :placeholder =>"Producing Agency Rate"  %>
        </td>
    <%else%>
        <td> <%= f.text_field :commission_rate_renew,:autocomplete => :off,:class=>"input-medium", :placeholder =>"Renewal Rate"  %>
        </td>
    <%end%>
    <td>
      <%= f.collection_select :billing_type, @paymenttype, :first, :last, {:include_blank => 'Select One'}, :class=>"input-medium" %>
    </td>





  </tr>
  </tbody>
</table>

