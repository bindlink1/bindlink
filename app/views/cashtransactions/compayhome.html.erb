<div class="col1wrap">
  <div class="col1 wellwhite">
    <div class="wellwhitemenu"><span class="headtext">Commissions Payable to Producing Agencies
    </span>
    </div>
    <div class="previewhead">



      <%= link_to "Process Commissions Payable", createcompaybatch_path, :class=>"btn btn-mini btn-success", :remote=>true, :id=>'processreturnbtn' %>


    </div>
    <div id='unprocessedbatches'>
      <%= render 'unprocessedcompaybatches' %>
    </div>
    <hr/>
        <div id='openbatches'>
      <%= render 'opencompaybatches' %>
    </div>
    <hr/>
    <h3>Processed Commission Payable Batches: Click to View</h3>
    <hr>

    <div class="dataTablewrapper">
      <table id="returnpremiumbatches" class="table table-bordered table-condensed ">
        <thead>
        <tr>
          <th>Action</th>
          <th>Batch Date</th>
          <th>Status</th>
        </tr>
        </thead>
        <tbody>
        <% if !@compaybatches.empty?  %>
            <% @compaybatches.each do |rb| %>
                <% begin %>
                    <tr >
                      <td><%= link_to 'view', viewcompaybatch_path(rb), :class=>"linktext"%> | <%= link_to 're-print checks', editcompaybatch_path(rb), :class=>"linktext"%></td>
                      <td><%= rb.created_at.strftime("%m/%d/%Y")  %></td>
                      <td><%= rb.status %></td>
                    </tr>

                <% end %>
            <% end %>
        <% else %>
            <tr>
              <td colspan="3">No batches just yet.</td>
            </tr>
        <% end  %>
        </tbody>
      </table>
    </div>


  </div>
</div>
