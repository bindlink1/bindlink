
<h3>Select an existing  <% if current_agent.isgeneralagent =="GA" %> policyholder <%else%> client <%end%></h3>

<div class="dataTablewrapper">
      <table id="clients" class="bordered-table zebra-striped ">
        <thead>
          <tr>
               <th>Client Name</th>
               <th>Address</th>
               <th>Created</th>
          </tr>
        </thead>
        <tbody>
            <% @allclients.each do |clients| %>
          <tr>
            <td><%= link_to clients.fullname.to_s , new_client_policy_path(clients)  %></td>
            <td><%= clients.address_1  %></td>
            <td><%= clients.created_at  %></td>
          </tr>
            <% end %>
        </tbody>
</table>
</div>

