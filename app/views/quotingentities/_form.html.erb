<%= form_for(@quotingentity) do |f| %>

  <div class="field">
   	<%= f.label :quotingentity_name %><br />
    <%= f.text_field :quotingentity_name %>
  </div>

  <div class="field">
    <%= f.label :address_1 %><br />
    <%= f.text_field :address_1%>
  </div>

  <div class="field">
    <%= f.label :address2 %><br />
    <%= f.text_field :address2%>
  </div>

  <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </div>

  <div class="field">
    <%= f.label :state %><br />
    <%= f.text_field :state %>
  </div>

  <div class="field">
    <%= f.label :zip %><br />
    <%= f.text_field :zip %>
  </div>

  <div class="field">
    <%= f.label :telephone %><br />
    <%= f.text_field :telephone %>
  </div>

  <div class="field">
    <%= f.label :principal %><br />
    <%= f.text_field :principal %>
  </div>

  <div class="field">
   Approved? </br>
    <%= label :approved, 'Yes' %>
    <%= f.radio_button :approved, "Yes" %>
    <%= label :approved, 'No' %>
    <%= f.radio_button :approved, "No" , :checked => true%>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>  
  
  
<% end %>
