    <h3>Unprocessed Return Premium Batches: Click to View</h3>
    <hr>

    <div class="dataTablewrapper">
      <table id="returnpremiumbatches" class="table table-bordered table-condensed ">
        <thead>
        <tr>
          <th>Action</th>
          <th>Batch Date</th>
          <th>Status</th>
        </tr>
        </thead>
        <tbody>
        <% if !@unprocessed.empty?  %>
            <% @unprocessed.each do |rb| %>
                <% begin %>
                    <tr >
                      <td><%= link_to 'view', returnlist_path(rb), :class=>"linktext"%>
                        | <%= link_to 'print invoices', printinvoicesunprocessed_path(rb), :class=>"linktext", :target=>"_blank"%>
                        | <%= link_to 'delete', deletereturnpremiumbatch_path(rb), :confirm=>"Are you sure you want to delete this batch?", :class=>"linktext"%></td>
                      <td><%= rb.created_at.strftime("%m/%d/%Y")  %></td>
                      <td><%= rb.status %></td>
                    </tr>

                <% end %>
            <% end %>
        <% else %>
            <tr>
              <td colspan="3">No batches just yet.</td>
            </tr>
        <% end  %>
        </tbody>
      </table>
    </div>