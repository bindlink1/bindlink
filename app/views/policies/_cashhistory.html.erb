</br>



<div>

  <div class="wellwhiteblock" style="padding-bottom: 20px">
    <div class="wellwhitemenu"><h3>Cash History</h3></div>
    <div class="row-fluid">

      <div class="span12">


        <table class="table table-striped table-bordered">
          <tr>
            <th>Date Entered</th>
            <th>Amount</th>
            <th>Type</th>
            <th>Check #</th>
          </tr>
          <% @cashhistoryforview.each do |cash| %>
              <tr>
                <td><%begin%> <%= cash.created_at.strftime("%m/%d/%Y") %> <%rescue%> <%end%></td>
                <td><%= number_to_currency(cash.cash_amount,  :negative_format => "(%u%n)")%></td>
                <td><%=cash.transaction_type.to_s%></td>
                 <td><%=cash.check_number.to_s%></td>
              </tr>
          <% end %>
        </table>


      </div>
    </div>
  </div>
</div>
