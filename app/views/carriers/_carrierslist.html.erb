<table class="table table-bordered table-layout:fixed;">
    <thead>
    <tr>
      <th colspan="4"><h3>Select a carrier to view more details or edit:</h3></th>
    </tr>
        <tr>
      <th>Name</th>
      <th>Appointed On</th>

    </tr>
    </thead>
    <tbody>
     <%  @carriers.each do |c| %>
       <tr>
         <td><%= link_to c.carrier_name.to_s , carrier_path(:id => c), :class=>"linktext" %></td>
         <td><%= c.created_at.strftime("%m/%d/%Y") %> </td>

       </tr>


     <% end %>
    </tbody>
</table>