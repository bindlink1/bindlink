<%= form_for @client,:html => {:autocomplete => "off", :class=>"form-horizontal"} do |f| %>

      <fieldset>
        <%= render :partial => 'formdetails', :locals=>{:f => f}%>
        <div class="form-actions">
          <%= f.submit 'Save Client', :class=>'btn  btn-success'%> <button class="btn closeit" onclick='return false;'>Go Back</button>
        </div>
      </fieldset>

<% end %>
