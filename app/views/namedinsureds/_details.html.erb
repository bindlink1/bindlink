<table class="table table-bordered table-condensed table-layout:fixed " id="producergentable">
  <col width="30%"  />
  <col width="70%" />
  <tr>
    <td style="text-align: right;">
      Producing Agency
    </td>
    <td style="text-align: left;">
     <%begin%> <%= link_to  @namedinsured.producingagency.agency_name, producingagency_path(@namedinsured.producingagency), :class=>'linktext' %>  <%rescue%> Bindlink Error: Migrate to Agency <%=@namedinsured.producingagency_id%> <%end%>
    </td>
  </tr>
  <tr>
    <td style="text-align: right;">
      Named Insured
    </td>
    <td style="text-align: left;">
      <%= @namedinsured.named_insured %>
    </td>
  </tr>

  <tr>
    <td style="text-align: right;">
      Address 1
    </td>
    <td style="text-align: left;">
      <%= @namedinsured.address_1 %>
    </td>
  </tr>
  <tr>
    <td style="text-align: right;">
      Address 2
    </td>
    <td style="text-align: left;">
      <%= @namedinsured.address_2 %>
    </td>
  </tr>
  <tr>
    <td style="text-align: right;">
      City
    </td>
    <td style="text-align: left;">
      <%= @namedinsured.city %>
    </td>
  </tr>
  <tr>
    <td style="text-align: right;">
      State
    </td>
    <td style="text-align: left;">
      <%= @namedinsured.state %>
    </td>
  </tr>
  <tr>
    <td style="text-align: right;">
      Zip
    </td>
    <td style="text-align: left;">
      <%= @namedinsured.zip %>
    </td>
  </tr>

</table>
