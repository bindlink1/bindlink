<fieldset>
  <div>
    <div class="control-group">
      <%= f.label :naic_number, :class=>"control-label"  %>
      <div class="controls">
        <%= f.text_field :naic_number,:class=>"input",:autocomplete => :off, :placeholder =>"NAIC Number"  %>
      </div>
    </div>

    <div class="form-actions">

      <%= f.submit 'Save', :remote => true,disable_with: 'Saving...', :class=>"btn btn-success ", :id=>"contactfrmbtn"%> <button class="btn " id="close_modal" onclick='return false;' >Cancel</button>


    </div>
  </div>
</fieldset>
