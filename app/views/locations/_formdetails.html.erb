
  <fieldset>

    <div class="control-group" id="firstname">
      <%= f.label :location_nickname, :class=>"control-label" %>

      <div class="controls">
        <%= f.text_field :location_nickname, :class=> "BLselect"   %>
      </div>
    </div>
    <div class="control-group" id="lastname">
      <%= f.label :address_1, :class=>"control-label" %>

      <div class="controls">
        <%= f.text_field :address_1, :class=> "BLselect"   %>
      </div>
    </div>
    <div class="control-group" id="corpname">
      <%= f.label :address_2, :class=>"control-label" %>
      <div class="controls">
        <%= f.text_field :address_2, :class=> "BLselect"   %>
      </div>
    </div>
    <div class="control-group">
      <%= f.label :city, :class=>"control-label" %>
      <div class="controls">
        <%= f.text_field :city, :class=> "BLselect"   %>
      </div>
    </div>
    <div class="control-group">
      <%= f.label :state, :class=>"control-label" %>
      <div class="controls">
        <%= f.collection_select :state, State.all, :abbreviation, :abbreviation, {:include_blank => 'Select One'} %>

      </div>
    </div>
    <div class="control-group">
      <%= f.label :zip , :class=>"control-label" %>
      <div class="controls">
        <%= f.text_field :zip, :class=> "BLselect"   %>
      </div>
    </div>
    <div class="control-group">
      <%= f.label :phone , :class=>"control-label" %>
      <div class="controls">
        <%= f.text_field :phone, :class=> "BLselect"   %>
      </div>
    </div>
    <div class="control-group">
      <%= f.label :fax , :class=>"control-label" %>
      <div class="controls">
        <%= f.text_field :fax, :class=> "BLselect"   %>
      </div>
    </div>

    <div class="form-actions">
      <%= f.submit 'Save Location', :class=>'btn  btn-success'%> <button class="btn" id="locationcancel" onclick='return false;'>Go Back</button>
    </div>
  </fieldset>


